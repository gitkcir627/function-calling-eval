    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: gpt",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/berkeley-function-call-leaderboard/",
            "args": [
                "--model", "gpt-4-0613",
                "--test_category", "rest"
            ]
        },
        {
            "name": "Python Debugger: llama2",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/berkeley-function-call-leaderboard/",
            "args": [
                "--model", "llama2",
                "--test_category", "executable_simple",
                "--temperature", "0.3"
            ]
        }
    ]
}